{
  "name": "tts-multi-api",
  "version": "1.0.0",
  "description": "Text to Speech dengan Multi API Key dan Parallel Processing",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx src/index.ts",
    "dev:web": "tsx src/web/server.ts",
    "build": "tsc",
    "build:web": "tsc && npm run build:frontend",
    "build:frontend": "vite build",
    "start": "node dist/index.js",
    "start:web": "node dist/web/server.js",
    "test": "vitest",
    "preview": "vite preview"
  },
  "keywords": [
    "tts",
    "text-to-speech",
    "parallel",
    "multi-api",
    "web"
  ],
  "author": "TTS Multi API",
  "license": "MIT",
  "dependencies": {
    "@google/genai": "^1.0.0",
    "archiver": "^6.0.0",
    "chalk": "^5.3.0",
    "commander": "^11.0.0",
    "compression": "^1.7.0",
    "cors": "^2.8.0",
    "express": "^4.18.0",
    "fluent-ffmpeg": "^2.1.3",
    "helmet": "^7.0.0",
    "mime": "^4.0.0",
    "multer": "^1.4.0",
    "ora": "^8.0.0",
    "socket.io": "^4.7.0"
  },
  "devDependencies": {
    "@types/archiver": "^6.0.0",
    "@types/compression": "^1.7.0",
    "@types/cors": "^2.8.0",
    "@types/express": "^4.17.0",
    "@types/fluent-ffmpeg": "^2.1.27",
    "@types/mime": "^3.0.0",
    "@types/multer": "^1.4.0",
    "@types/node": "^20.0.0",
    "tsx": "^4.0.0",
    "typescript": "^5.0.0",
    "vite": "^5.0.0",
    "vitest": "^1.0.0"
  }
}
